{
  "name": "emojiq",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "start": "op run --env-file=\"./.env.development\" -- node dist/bin/www.js",
    "dev": "op run --env-file=\"./.env.development\" -- nodemon --exec ts-node -r tsconfig-paths/register ./bin/www.ts",
    "migrate": "op run --env-file=\"./.env.development\" -- knex --knexfile ./knexfile.ts migrate:latest"
  },
  "dependencies": {
    "@slack/web-api": "^7.12.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "emoji-regex": "^10.6.0",
    "emojilib": "^4.0.2",
    "express": "~4.22.0",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "knex": "^3.1.0",
    "morgan": "~1.9.1",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.9",
    "@types/debug": "^4.1.12",
    "@types/express": "^5.0.3",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.6.2",
    "@types/pg": "^8.15.6",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  }
}
